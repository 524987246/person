@charset "UTF-8";

body,h1,ul {
	margin: 0;
	padding: 0;
	background-color: #333;
}
ul {
	list-style: outside none none;
}
a {
	text-decoration: none;
}
#nav {
	width: 100%;
	height: 100%;
	
}
#nav .center {
	width: 100%;
	height: 100%;
	margin: 0 auto;
}
#nav .logo {
	width: 240px;
	height: 70px;
	text-indent: -9999px;
	float: left;
}
#nav .link {
	width: 650px;
	height: 100%;
	line-height: 70px;
	color: #333;
	float: right;
}
#nav .link li {
	width: 120px;
	text-align: center;
	float: left;
}
#nav .link a {
	color: #eee;
	display: block;
}
#nav .link a:hover, 
#nav .active a {
	background-color: #000;
}