package org.great.web.bean;

import org.springframework.context.annotation.Scope;
import org.springframework.stereotype.Component;


/**
 * 用户bean
 * @author 谢军
 *
 */
@Component
@Scope("prototype")
public class User {
	private int user_id;
	private String password;
	private String user_name;
	private String user_nike;
	private String user_sex;
	private int user_age;
	private String birthday;
	private String mobilephone;
	private String identity_card;
	private int height;
	private int weight;
	private int income;// 收入
	private String province;// 省
	private String city;// 市
	private String area;// 区
	private String horoscope;// 星座
	private String introduce;//自我介绍
	private String edu_background;//学历
	private String industry;//行业
	private String is_text_passed;//文本信息是否通过审核
	private String text_passed_cont;//文本信息的审核结果
