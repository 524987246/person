	private int isForbidden;// 是否禁用账号
	@Override
	public String toString() {
		return "User [user_id=" + user_id + ", password=" + password
				+ ", user_name=" + user_name + ", user_nike=" + user_nike
				+ ", user_sex=" + user_sex + ", user_age=" + user_age + ", birthday="
				+ birthday + ", mobilephone=" + mobilephone
				+ ", identity_card=" + identity_card + ", height=" + height
				+ ", weight=" + weight + ", income=" + income + ", province="
				+ province + ", city=" + city + ", area=" + area+ ", horoscope=" + horoscope
				+ ", user_level=" + user_level + ", head_path=" + head_path
				+ "]";
	}
	public int getUser_id() {
		return user_id;
	}

	public void setUser_id(int user_id) {
		this.user_id = user_id;
	}

	public String getPassword() {
		return password;
	}

	public String getText_passed_cont() {
		return text_passed_cont;
	}
	public void setText_passed_cont(String text_passed_cont) {
		this.text_passed_cont = text_passed_cont;
	}
	public String getIntroduce() {
		return introduce;
	}
	public void setIntroduce(String introduce) {
		this.introduce = introduce;
	}
	public void setPassword(String password) {
		this.password = password;
	}


	public String getHead_passed_cont() {
		return head_passed_cont;
	}
	public void setHead_passed_cont(String head_passed_cont) {
		this.head_passed_cont = head_passed_cont;
	}
	public String getUser_name() {
		return user_name;
	}

	public void setUser_name(String user_name) {
		this.user_name = user_name;
	}


	public String getUser_nike() {
		return user_nike;
	}

	public void setUser_nike(String user_nike) {
		this.user_nike = user_nike;
	}

